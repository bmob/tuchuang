{
    "name": "图床@Bmob",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "launch": {
            "local_path": "popup.html"
        }
    },
    "icons": {
        "64": "image/icon_64.png",
        "32": "image/icon_32.png"
    },
    "offline_enabled": true,
    "externally_connectable": {
        "ids": [ "*" ]
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "unlimitedStorage",
        "storage"
    ]
}